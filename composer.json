{
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.13"
    },
    "scripts": {
        "test": [
          "vendor/bin/phpunit ./tests"
        ],
        "test-round1": [
          "vendor/bin/phpunit ./tests --group round1"
        ],
        "test-round2": [
          "vendor/bin/phpunit ./tests --group round2"
        ],
        "test-round3": [
          "vendor/bin/phpunit ./tests --group round3"
        ],
        "lint": [
          "vendor/bin/php-cs-fixer fix"
        ]
    }
}
